        <errorConditionFormula>CONTAINS( Country__c , &quot;\&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Country__c</errorDisplayField>
        <errorMessage>quotes are not allowed in address formats</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_have_Email_for_Reminder</fullName>
