        <errorDisplayField>Reminder_Method__c</errorDisplayField>
        <errorMessage>No Receipient Email exist. Please choose another Reminder Method.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_have_mobile_for_reminder</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Reminder_Method__c , &quot;SMS&quot;) &amp;&amp;  ISBLANK(Recipient_Mobile__c) &amp;&amp;  $Profile.Name &lt;&gt; &quot;SEW Administrator&quot; &amp;&amp; $Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp; $Profile.Name &lt;&gt; &quot;SEW Integration&quot;</errorConditionFormula>
        <errorDisplayField>Reminder_Method__c</errorDisplayField>
        <errorMessage>No Receipient Mobile exist. Please choose another Reminder Method.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_New_Billing_Account</fullName>
        <active>true</active>
        <description>Not allow Non-SEW Misc Charge users to create Group Accounts.</description>
        <errorConditionFormula>$RecordType.DeveloperName=&quot;Group Billing Account &quot;&amp;&amp; ISNEW()&amp;&amp;  $Profile.Name  &lt;&gt; &quot;SEW Misc Charges&quot;</errorConditionFormula>
        <errorMessage>You can&apos;t create a new Billing Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Post_Code_Special_Characters</fullName>
        <active>true</active>
        <description>quotes and periods are not allowed in address formats</description>
