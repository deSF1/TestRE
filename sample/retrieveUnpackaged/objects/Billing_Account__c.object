        <errorDisplayField>Postal_Code__c</errorDisplayField>
        <errorMessage>quotes are not allowed in address formats</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_Group_Account</fullName>
        <active>true</active>
