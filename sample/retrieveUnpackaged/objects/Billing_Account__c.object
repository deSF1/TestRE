    </validationRules>
    <validationRules>
        <fullName>eBill_Method_BPAY_change</fullName>
        <active>true</active>
        <description>Cannot change the eBill Method from or to BPAY.
But allow system adminstrators to do this, for integration.</description>
        <errorConditionFormula>AND(
ISCHANGED( eBill_Method__c ) ,
 OR(
  ISPICKVAL(eBill_Method__c , &quot;BPAY View&quot;) ,
 ISPICKVAL(PRIORVALUE (eBill_Method__c ),  &quot;BPAY View&quot;)
) 
)
&amp;&amp; (NOT($Profile.Name=&quot;System Administrator&quot; || $Profile.Name=&quot;SEW Administrator&quot; || $Profile.Name=&quot;SEW Integration&quot; ))</errorConditionFormula>
        <errorDisplayField>eBill_Method__c</errorDisplayField>
        <errorMessage>Cannot change eBill Method from or to BPAY.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>eBill_Recipient_Check</fullName>
        <active>true</active>
        <description>eBill Recipient must be entered if the eBill Method is email or Reminder is SMS.</description>
        <errorConditionFormula>AND(
ISBLANK( eBill_Recipient__c ),
 (OR(ISPICKVAL(eBill_Method__c  , &quot;Email&quot;),
 ISPICKVAL( Reminder_Method__c , &quot;SMS&quot;)) )
)</errorConditionFormula>
        <errorDisplayField>eBill_Recipient__c</errorDisplayField>
        <errorMessage>An eBill Recipient must be entered if the eBill Method is email or Reminder is SMS.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>eBill_Recipient_email_address</fullName>
        <active>true</active>
        <description>Check that the ebill recipient has an email address.</description>
        <errorConditionFormula>AND( ISPICKVAL( eBill_Method__c , &quot;Email&quot;) ,
 ISBLANK( eBill_Recipient__r.Email ) 
)</errorConditionFormula>
        <errorDisplayField>eBill_Recipient__c</errorDisplayField>
        <errorMessage>eBill Recipient must have an email address for this eBill Method.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>eBill_Recipient_mobile_check</fullName>
        <active>true</active>
        <description>Check that the ebill recipient has a mobile number.</description>
        <errorConditionFormula>AND( ISPICKVAL(  Reminder_Method__c  , &quot;SMS&quot;) ,
ISBLANK(  eBill_Recipient__r.MobilePhone )
)</errorConditionFormula>
        <errorDisplayField>eBill_Recipient__c</errorDisplayField>
        <errorMessage>eBill Recipient must have a mobile number for this Reminder Method.</errorMessage>
    </validationRules>
