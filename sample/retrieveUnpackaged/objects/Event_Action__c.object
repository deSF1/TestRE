    <webLinks>
        <fullName>Publish</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Publish</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/26.0/connection.js&apos;)} 
{!REQUIRESCRIPT(&apos;/soap/ajax/26.0/apex.js&apos;)}

if (&apos;{!Event_Action__c.Publish_status__c}&apos; == &apos;Published&apos;){
	alert(&apos;Event already published&apos;);
}else if (&apos;{!Event_Action__c.Publish_status__c}&apos; == &apos;Closed&apos;){
	alert(&apos;Closed event cannot be published.&apos;);
}else{
var eventAction = new sforce.SObject(&quot;Event_Action__c&quot;);
	eventAction.Id = &apos;{!Event_Action__c.Id}&apos;;
	
	eventAction.Publish_Status__c = &apos;Published&apos;;
	
	sforce.connection.update([eventAction]);
        window.location.reload();
}</url>
    </webLinks>
    <webLinks>
        <fullName>Resolve</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Resolve</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/26.0/connection.js&apos;)}
{!REQUIRESCRIPT(&apos;/soap/ajax/26.0/apex.js&apos;)}

if (&apos;{!Event_Action__c.Publish_status__c}&apos; != &apos;Published&apos;){
	alert(&apos;Cannot resolve as the event is not currently published&apos;);
}else{
	var parentEvent = new sforce.SObject(&quot;SEW_Event__c&quot;);
	
	parentEvent.Id = &apos;{!Event_Action__c.SEW_EventId__c}&apos;;
	parentEvent.Public_Status__c = &apos;Resolved&apos;;
	sforce.connection.update([parentEvent]);
	
	window.location.reload();
}</url>
    </webLinks>
