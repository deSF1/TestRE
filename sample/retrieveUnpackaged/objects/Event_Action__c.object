{!REQUIRESCRIPT(&apos;/soap/ajax/26.0/apex.js&apos;)}
{!REQUIRESCRIPT(&quot;/xdomain/xdomain.js&quot;)}


if (&apos;{!Event_Action__c.Publish_status__c}&apos; == &apos;Closed&apos;){
